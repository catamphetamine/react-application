{
  "name": "react-pages",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "default": "./index.js"
    },
    "./server": {
      "default": "./parse.js"
    },
    "./client": {
      "default": "./client.js"
    },
    "./static-site-generator": {
      "default": "./static-site-generator.js"
    }
  },
  "version": "0.6.12",
  "description": "A simple toolkit for building a React/Redux application: routing, loading page data, fetching data over HTTP, (optional) server-side rendering, etc.",
  "dependencies": {
    "@catamphetamine/found": "^1.1.2",
    "@catamphetamine/found-scroll": "^1.1.0",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "core-js-pure": "^3.19.1",
    "fs-extra": "^10.0.0",
    "lodash-es": "^4.17.21",
    "multistream": "^4.1.0",
    "nunjucks": "^3.2.3",
    "print-error": "^0.1.17",
    "progress": "^2.0.3",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "^2.13.9",
    "string-to-stream": "^3.0.1",
    "superagent": "^7.1.2"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "core-js": "^3.19.1",
    "crlf": "^1.1.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0"
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-dom": ">= 18",
    "redux": ">= 4.2.0",
    "react-redux": ">= 8"
  },
  "resolutions": {
    "found/react-redux": ">= 8"
  },
  "overrides": {
    "found": {
      "react-redux": ">= 8"
    }
  },
  "scripts": {
    "test": "mocha --bail --require ./test/setup.js \"lib/**/*.test.js\" \"test/**/*.test.js\"",
    "test-coverage": "nyc mocha --bail --require ./test/setup.js \"lib/**/*.test.js\" \"test/**/*.test.js\"",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/catamphetamine/react-pages.git"
  },
  "keywords": [
    "react",
    "pages",
    "framework",
    "server",
    "ssr"
  ],
  "author": "catamphetamine <purecatamphetamine@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/catamphetamine/react-pages/issues"
  },
  "homepage": "https://gitlab.com/catamphetamine/react-pages#readme"
}
